   "source": [
    "#### Swapping Varibles"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "X is 30\n",
      "y is 20\n"
     ]
    }
   ],
   "source": [
    "x, y = 20, 30\n",
    "\n",
    "x, y = y, x\n",
    "\n",
    "print('X is', x)\n",
    "print('y is', y)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Swap data using XOR operator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "x is: 30\n",
      "y is: 20\n"
     ]
    }
   ],
   "source": [
    "x = 20\n",
    "y = 30\n",
    "\n",
    "# Step one\n",
    "x ^= y\n",
    "\n",
    "# Step two\n",
    "y ^= x\n",
    "\n",
    "# Step three\n",
    "x ^= y\n",
    "\n",
    "print(f'x is: {x}')\n",
    "print(f'y is: {y}')"
   ]
  },
